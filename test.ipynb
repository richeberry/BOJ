   "source": [
    "# x = int(input())\n",
    "\n",
    "x = 10\n",
    "\n",
    "dp = [0] * (x + 1)\n",
    "\n",
    "# dp[2] = 1\n",
    "# dp[3] = 1\n",
    "\n",
    "\n",
    "for i in range(2, x+1):\n",
    "    dp[i] = dp[i-1] + 1\n",
    "\n",
    "    print(dp)\n",
    "\n",
    "    if i % 2 == 0:\n",
    "        dp[i] = min(dp[i], dp[i//2] + 1)\n",
    "    if i % 3 == 0:\n",
    "        dp[i] = min(dp[i], dp[i//3] + 1)\n",
    "\n",
    "print(dp[x])"
   ]
  }
